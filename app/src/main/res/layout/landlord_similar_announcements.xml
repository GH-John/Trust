<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">

    <com.application.arenda.ui.widgets.horizontalList.HorizontalList
        android:id="@+id/landLordAnnouncements"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:titleSeeAll="@string/title_announcements_landlord" />

    <com.application.arenda.ui.widgets.horizontalList.HorizontalList
        android:id="@+id/similarAnnouncements"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:titleSeeAll="@string/title_announcements_similar" />
</LinearLayout>